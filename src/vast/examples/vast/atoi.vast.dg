fun atoi(value: string): i32 {
    var result = 0;
    for (ch in value) {
        result = result + when(ch.charCode()) {
            '0'.charCode()..('9'.charCode()) -> result = result * 10 + ch.charCode() - '0'.charCode();
            else -> break;
        };
    };
    result
}

val b30 = "30";
val b12 = "12";

fun Test(): i32 {
    atoi(value: b30) + atoi(value: b12)
}